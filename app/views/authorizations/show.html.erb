<%= render partial: 'authorization_code', locals: { authorization_code: @authorization_code } %>

<div>
  <%= link_to "Back to authorizations", authorizations_path %>

  <% unless @authorization_code.access_token_expired? %>
    <%= button_to "Revoke this token", @authorization_code.access_token, method: :delete %>
  <% end %>
</div>
