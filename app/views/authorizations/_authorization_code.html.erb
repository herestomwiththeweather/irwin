<div id="<%= dom_id authorization_code %>" class="m-2 px-50" style="background-color: rgb(241 245 249);">
    <img src="<%= authorization_code.client_app.logo_url %>" style="display: inline; padding: 2px;" width=100 height=100>
    <span class="mr-3"><%= link_to authorization_code.created_at.to_s(:rfc822), authorization_path(authorization_code) %></span><span class="mr-3"><a href="<%= authorization_code.client_id %>"><%= authorization_code.client_id %></a></span><span class="font-bold"><%= authorization_code.scope %></span>
</div>
