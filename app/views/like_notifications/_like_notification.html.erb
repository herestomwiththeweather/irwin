<%= turbo_frame_tag(dom_id like_notification) do %>
  <div><%= image_tag like_notification.account.icon_url_or_default, style: 'display: inline; padding: 2px; max-width: 50px; max-height: 50px;' %><b><%= link_to like_notification.account.name, like_notification.account, data: {'turbo-frame': '_top'} %></b> liked <%= link_to like_notification.status.id, like_notification.status, data: {'turbo-frame': '_top'} %> <%= time_ago_in_words(like_notification.created_at) %> ago</div>
<% end %>
